            <artifactId>provider-api-login-log-admin</artifactId>
            <version>${project.parent.version}</version>
        </dependency>
    </dependencies>

    <build>
        <plugins>
            <plugin>
                <groupId>org.springframework.boot</groupId>
                <artifactId>spring-boot-maven-plugin</artifactId>
                <configuration>
                    <mainClass>com.misakiga.husky.message.MessageAdminLoginLogBootstrap</mainClass>
                </configuration>
            </plugin>
        </plugins>
